/*Write a Java program to print the result of the following operations. */
class Q4
{
	public static void main(String args[])
	{
		System.out.println("Calculate the following expressions");
		System.out.println("a. -5+8*6");
		int i =-5+8*6;
		System.out.println("Result = "+i);
		
		System.out.println("b. (55+9)%9");
		int i1 =(55+9)%9;
		System.out.println("Result = "+i1);
		
		System.out.println("c. 20+ -3*5/8");
		int i2 =20+ -3*5/8;
		System.out.println("Result = "+i2);
		
		System.out.println("d. 5+15/3*2-8%3");
		int i3 =5+15/3*2-8%3;
		System.out.println("Result = "+i3);	
	}
}
