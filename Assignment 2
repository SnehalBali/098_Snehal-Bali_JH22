/*.  Write a Java program to print an American flag on the screen. */ 

//import java.util.*;
class Q14
{
	public static void main(String args[])
	{
		System.out.println("* * * * * * * =============================");
		System.out.println(" * * * * * *  =============================");
		System.out.println("* * * * * * * =============================");
		System.out.println(" * * * * * *  =============================");
		System.out.println("* * * * * * * =============================");
		System.out.println(" * * * * * *  =============================");
		System.out.println("* * * * * * * =============================");
		System.out.println(" * * * * * *  =============================");
		System.out.println("* * * * * * * =============================");
		System.out.println("===========================================");
		System.out.println("===========================================");
		System.out.println("===========================================");
		System.out.println("===========================================");
		System.out.println("===========================================");
		System.out.println("===========================================");

	}
}	
