/*.   Write a Java program to print a face. */ 

//import java.util.*;
class Q16
{
	public static void main(String args[])
	{
		System.out.println(" +\"\"\"\"\"+ ");
		System.out.println("[| o o |]");
		System.out.println(" |  ^  | ");
		System.out.println(" | ' ' | ");
		System.out.println(" |  -  | ");
		System.out.println(" +-----+ ");
		
	}
}	
